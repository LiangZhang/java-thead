/**
 * @author likly
 * @version 1.0
 */
public class Person {
    private String name;
    private int age;
    static int sex;
    private int avg;


    public Person() {
        System.out.println("person");
    }

    public Person(String name, int age) {
        this.name = name;
        this.age = age;
    }

    private Person(String name, int age,int avg) {
        this.name = name;
        this.age = age;
        this.avg = avg;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public int getAvg() {
        return avg;
    }

    public void setAvg(int avg) {
        this.avg = avg;
    }

    @Override
    public String toString() {
        return "Person{" +
                "name='" + name + '\'' +
                ", age=" + age +
                ", avg=" + avg +
                '}';
    }
}
    
    