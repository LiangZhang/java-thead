public class bigDecimalCompare {
    public static void main(String[] args) {

        BigDecimal a=BigDecimal.valueOf(0.0);
        BigDecimal b=BigDecimal.valueOf(0.000);
        BigDecimal c = BigDecimal.valueOf(0);

        System.out.println(a.compareTo(b) == 0);
        System.out.println(a.compareTo(c) == 0);


        BigDecimal d = new BigDecimal("0.0");
        BigDecimal e = new BigDecimal("0.000");
        BigDecimal f = new BigDecimal("0");

        System.out.println(d.compareTo(e) == 0);
        System.out.println(d.compareTo(f) == 0);

        System.out.println(a.compareTo(d) == 0);
        System.out.println(a.compareTo(f) == 0);
    }
}